<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>GoogleMail_GPL</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries>
                <library>
                    <file>${libs.Portlet-1.0-Lib.classpath}</file>
                </library>
                <library>
                    <file>${libs.Rome.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${libs.Liferay-5.1.2.classpath}</file>
                </library>
                <library>
                    <file>${libs.JDOM.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${libs.GData.classpath}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
    </configuration>
</project>
